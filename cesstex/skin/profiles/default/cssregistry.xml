<?xml version="1.0"?>
<object name="portal_css">

    <stylesheet title="CESSTEX Main Style"
                id="cesstex.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                expression=""/>

    <stylesheet title="HOME-PAGE-FLASH Style"
                id="home_page.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                expression="object/isHomePage"/>

    <stylesheet title=""
                id="formulaire_style.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                expression=""/>

    <stylesheet title="ISM Styles"
                id="++theme++cesstex.skin/ism/css/style.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="request/HTTP_X_THEME_ENABLED | nothing"/>

</object>
